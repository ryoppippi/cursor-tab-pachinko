{
	"name": "cursor-tab-pachinko",
	"version": "0.0.5",
	"license": "MIT",
	"bin": "./dist/index.mjs",
	"files": [
		"dist"
	],
	"type": "module",
	"exports": {
		".": "./dist/index.mjs",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "bun -b tsdown",
		"lint": "bun run lint:oxfmt && bun run lint:oxlint",
		"lint:oxfmt": "oxfmt --check .",
		"lint:oxlint": "oxlint --max-warnings=0 --type-aware .",
		"format": "bun run format:oxfmt && bun run format:oxlint",
		"format:oxfmt": "oxfmt --write .",
		"format:oxlint": "oxlint --fix --type-aware .",
		"release": "bumpp && bun run build"
	},
	"dependencies": {
		"open": "^11.0.0"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"bumpp": "^10.4.0",
		"publint": "^0.3.16",
		"tsdown": "^0.20.0-beta.4",
		"unplugin-unused": "^0.5.6"
	}
}
